<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:paddingRight="@dimen/dimen_10dp"
    android:paddingLeft="@dimen/dimen_10dp" >

    <ImageView
        android:layout_width="@dimen/dimen_13dp"
        android:layout_height="@dimen/dimen_13dp"
        android:id="@+id/iv_time"
        android:layout_centerVertical="true"
        android:src="@drawable/srch_icn_clock"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/dimen_13sp"
        android:layout_toRightOf="@id/iv_time"
        android:layout_centerVertical="true"
        android:singleLine="true"
        android:ellipsize="end"
        android:layout_marginLeft="@dimen/dimen_10dp"
        android:textColor="@color/text_color_main"
        android:text=""
        android:id="@+id/tv_histroy"/>

    <ImageView
        android:layout_width="@dimen/dimen_13dp"
        android:layout_height="@dimen/dimen_13dp"
        android:layout_alignParentRight="true"
        android:id="@+id/im_delete"
        android:layout_centerVertical="true"
        android:src="@drawable/list_icn_delete"/>

    <View
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="@dimen/dimen_23dp"
        style="@style/DivideLineHorizontal"></View>

</RelativeLayout>